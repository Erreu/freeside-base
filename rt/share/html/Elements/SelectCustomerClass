% return if ($RT::URI::freeside::IntegrationType ne 'Internal');
<select name="<%$Name%>">
% if ($ShowNullOption) {
  <option value="">-</option>
% }
% for my $class (qsearch('cust_class', {'disabled' => ''})) {
  <option value="<%$class->classnum%>" <% 
  $class->classnum == $Default||'' ? 'selected' : ''%>
  ><%$class->classname%></option>
% }
</select>
<%init></%init>
<%args>
$ShowNullOption => 1
$Name => undef
$Default => 0
</%args>

use strict;
require ('@RT_BIN_PATH@/webmux.pl');

my $h = &RT::Interface::Web::NewCGIHandler();

require CGI;

RT::Init();

my $cgi = CGI->new;
unless ($h->interp->comp_exists($cgi->path_info)) {
    $cgi->path_info($cgi->path_info . "/index.html");
}
$h->handle_cgi_object($cgi);

1;
